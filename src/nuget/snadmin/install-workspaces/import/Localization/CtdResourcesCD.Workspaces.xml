        </data>
        <data name="EmailField-DisplayName" xml:space="preserve">
          <value>Email field</value>
        </data>
        <data name="EmailField-Description" xml:space="preserve">
          <value>The name of the field specified among ContentList fields containing the submitter's email address. Ie.: "#Email" or "#SubmitterEmail".</value>
        </data>
