<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="view.xslt"?>
<Resources>
  <ResourceClass name="Ctd-Post">
    <Languages>
      <Language cultureName="en">
        <data name="DisplayName" xml:space="preserve">
          <value>Post</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Represents wall posts.</value>
        </data>
        <data name="JournalId-DisplayName" xml:space="preserve">
          <value>Journal Id</value>
        </data>
        <data name="JournalId-Description" xml:space="preserve">
          <value>Id of the corresponding journal item. 0 if the post is not derived from a journalitem.</value>
        </data>
        <data name="PostType-DisplayName" xml:space="preserve">
          <value>Post type</value>
        </data>
        <data name="PostType-Description" xml:space="preserve">
          <value>Type of post item ie. Manual, Created, Modified, etc.</value>
        </data>
        <data name="SharedContent-DisplayName" xml:space="preserve">
          <value>Shared Content</value>
        </data>
        <data name="SharedContent-Description" xml:space="preserve">
          <value>The shared Content if it has been attached to the post.</value>
        </data>
        <data name="PostDetails-DisplayName" xml:space="preserve">
          <value>Post details</value>
        </data>
        <data name="PostDetails-Description" xml:space="preserve">
          <value>Contains details of journal posts.</value>
        </data>
      </Language>
      <Language cultureName="hu">
        <data name="DisplayName" xml:space="preserve">
          <value>Poszt</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Poszt, ami a falon megjelenik.</value>
        </data>
        <data name="JournalId-DisplayName" xml:space="preserve">
          <value>Journal azonosító</value>
        </data>
        <data name="JournalId-Description" xml:space="preserve">
          <value>A hozzátartozó journal elem azonosítója. Értéke nulla (0) ha a poszt nem kapcsolódik a journal-hoz.</value>
        </data>
        <data name="PostType-DisplayName" xml:space="preserve">
          <value>Poszt típusa</value>
        </data>
        <data name="PostType-Description" xml:space="preserve">
          <value>Manuális, létrehozás, módosítás, stb.</value>
        </data>
        <data name="SharedContent-DisplayName" xml:space="preserve">
          <value>Megosztott tartalom</value>
        </data>
        <data name="SharedContent-Description" xml:space="preserve">
          <value>A megosztott tartalom, ha volt olyan kapcsolva a poszthoz.</value>
        </data>
        <data name="PostDetails-DisplayName" xml:space="preserve">
          <value>Poszt részletei</value>
        </data>
        <data name="PostDetails-Description" xml:space="preserve">
          <value></value>
        </data>
      </Language>
    </Languages>
  </ResourceClass>
  <ResourceClass name="Ctd-Posts">
    <Languages>
      <Language cultureName="en">
        <data name="DisplayName" xml:space="preserve">
          <value>Posts</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>This is the container type for Posts.</value>
        </data>
      </Language>
      <Language cultureName="hu">
        <data name="DisplayName" xml:space="preserve">
          <value>Posztok</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Posztok gyüjtője.</value>
        </data>
      </Language>
    </Languages>
  </ResourceClass>
  <ResourceClass name="Ctd-ProjectWorkspace">
    <Languages>
      <Language cultureName="en">
        <data name="DisplayName" xml:space="preserve">
          <value>Project workspace</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>A workspace for project-oriented cooperation.</value>
        </data>
        <data name="Completion-DisplayName" xml:space="preserve">
          <value>Completion</value>
        </data>
        <data name="ClientContact-DisplayName" xml:space="preserve">
          <value>Client contact</value>
        </data>
        <data name="ClientContact-Description" xml:space="preserve">
          <value>The person who is responsible for the project</value>
        </data>
        <data name="WebsiteUrl-DisplayName" xml:space="preserve">
          <value>Website url</value>
        </data>
      </Language>
      <Language cultureName="hu">
        <data name="DisplayName" xml:space="preserve">
          <value>Projekt munkaterület</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Projektek kezelésére szolgáló munkaterület (workspace).</value>
        </data>
        <data name="Completion-DisplayName" xml:space="preserve">
          <value>Készültség</value>
        </data>
        <data name="ClientContact-DisplayName" xml:space="preserve">
          <value>Ügyféloldali projektfelelős</value>
        </data>
        <data name="ClientContact-Description" xml:space="preserve">
          <value></value>
        </data>
        <data name="WebsiteUrl-DisplayName" xml:space="preserve">
          <value>Weboldal url-je</value>
        </data>
      </Language>
    </Languages>
  </ResourceClass>
  <ResourceClass name="Ctd-ProjectWorkspaceFolder">
    <Languages>
      <Language cultureName="en">
        <data name="DisplayName" xml:space="preserve">
          <value>Project Workspace Folder</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>This is the container type for Project Workspace.</value>
        </data>
      </Language>
      <Language cultureName="hu">
        <data name="DisplayName" xml:space="preserve">
          <value>Projekt munkaterület mappa</value>
        </data>
        <data name="Description" xml:space="preserve">
          <value>Projekt munkaterületek gyüjtője.</value>
        </data>
      </Language>
    </Languages>
  </ResourceClass>
</Resources>
